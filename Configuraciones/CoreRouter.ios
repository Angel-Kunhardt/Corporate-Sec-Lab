en
conf t
hostname CoreRouter
no ip domain lookup
enable secret class
line console 0
password LabSec2025
login
line vty 0 4
password LabSec2025
transport input ssh
login
banner motd $--- Unauthorized Access is Strictly Forbidden ---$
service password-encryption
banner motd $Solo Usuarios autorizados$
username SSHadmin secret LabSec2025
ip domain name LabSec.Dev
crypto key generate rsa general-keys modulus 1024

ip route 0.0.0.0 0.0.0.0 172.16.0.9

int vlan 1
ip add 172.16.1.2 255.255.255.240
no sh
exit

int g0/0
ip add 172.16.0.10 255.255.255.252
description CoreRouter->Firewall
no sh
exit

int fa2/0
no ip add
description CoreRouter->DataCenter
no sh

int fa2/1
no ip add
description CoreRouter->Core-SW
no sh

int fa3/0
no ip add
description CoreRouter->IT-SW
no sh

router ospf 10
router-id 10.10.10.10
passive-interface default
network 172.16.1.0 0.0.0.3 area 0
network 172.16.10.0 0.0.0.255 area 0
network 172.16.20.0 0.0.0.255 area 0
network 172.16.21.0 0.0.0.255 area 0
network 172.16.22.0 0.0.0.255 area 0
network 172.16.23.0 0.0.0.255 area 0

int fa2/0.10
encapsulation dot1q 10
ip add 172.16.10.1 255.255.255.0
description DataCenter
ip ospf 10 area 0